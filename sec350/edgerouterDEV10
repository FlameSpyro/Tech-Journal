set firewall name LAN-to-DMZ rule 10 source address '172.16.150.10'
set firewall name LAN-to-DMZ rule 11 action 'accept'
set firewall name LAN-to-DMZ rule 11 description 'Traffic flow from Lan to DMZ'
set firewall name LAN-to-DMZ rule 11 destination address '172.16.50.1-172.16.50.6'
set firewall name LAN-to-DMZ rule 11 destination port '80'
set firewall name LAN-to-DMZ rule 11 protocol 'tcp'
set firewall name LAN-to-WAN default-action 'drop'
set firewall name LAN-to-WAN enable-default-log
set firewall name LAN-to-WAN rule 1 action 'accept'
set firewall name WAN-to-DMZ default-action 'drop'
set firewall name WAN-to-DMZ description 'Allow http from WAN to DMZ'
set firewall name WAN-to-DMZ enable-default-log
set firewall name WAN-to-DMZ rule 1 action 'accept'
set firewall name WAN-to-DMZ rule 10 action 'accept'
set firewall name WAN-to-DMZ rule 10 description 'Allow Port 80'
set firewall name WAN-to-DMZ rule 10 destination address '172.16.50.3'
set firewall name WAN-to-DMZ rule 10 destination port '80'
set firewall name WAN-to-DMZ rule 10 protocol 'tcp'
set firewall name WAN-to-DMZ rule 10 source
set firewall name WAN-to-DMZ rule 30 action 'accept'
set firewall name WAN-to-DMZ rule 30 description 'WAN to ssh to jump'
set firewall name WAN-to-DMZ rule 30 destination address '172.16.50.4'
set firewall name WAN-to-DMZ rule 30 destination port '22'
set firewall name WAN-to-DMZ rule 30 protocol 'tcp'
set firewall name WAN-to-LAN default-action 'drop'
set firewall name WAN-to-LAN enable-default-log
set firewall name WAN-to-LAN rule 1 action 'accept'
set firewall name WAN-to-LAN rule 1 state established 'enable'
